<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- alles erlaubt, außer … -->
        <exclude domain="sharedpref" path="prefs.xml"/>
        <exclude domain="sharedpref" path="secure_auth_prefs.xml"/>
        <exclude domain="sharedpref" path="secure_auth_prefs__androidx_security_crypto_encrypted_prefs_value_keyset.xml"/>
        <exclude domain="sharedpref" path="secure_auth_prefs__androidx_security_crypto_encrypted_prefs_key_keyset.xml"/>
